      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/material-web/utils/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/material-web/utils"],
      "options": {
        "jestConfig": "libs/material-web/utils/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["utils"]
}
